package app18;
import java.util.*;

public class Ifelse_Demo2 {

	public static void main(String[] args) {
		int a,b,max;
		Scanner sc=new Scanner(System.in);
		System.out.println("Max Value Is");
		a=sc.nextInt();
		b=sc.nextInt();
		if (a>b) {
			System.out.println("Max"+a);
		}
		else {
			System.out.println("Max"+b);
			
		}

	}

}
